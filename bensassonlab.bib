@article{clowers_ecological_2015,
	title = {Ecological and genetic barriers differentiate natural populations of {Saccharomyces} cerevisiae},
	issn = {0737-4038, 1537-1719},
	url = {http://mbe.oxfordjournals.org/content/early/2015/05/06/molbev.msv112},
	doi = {10.1093/molbev/msv112},
	abstract = {How populations that inhabit the same geographical area become genetically differentiated is not clear. To investigate this, we characterized phenotypic and genetic differences between two populations of Saccharomyces cerevisiae that in some cases inhabit the same environment but show relatively little gene flow. We profiled stress sensitivity in a group of vineyard isolates and a group of oak-soil strains and found several niche-related phenotypes that distinguish the populations. We performed bulk-segregant mapping on two of the distinguishing traits: the vineyard-specific ability to grow in grape juice and oak-specific tolerance to the cell wall damaging drug Congo red. To implicate causal genes, we also performed a chemical genomic screen in the lab-strain deletion collection and identified many important genes that fell under QTL peaks. One gene important for growth in grape juice and identified by both the mapping and the screen was SSU1, a sulfite-nitrite pump implicated in wine fermentations. The beneficial allele is generated by a known translocation that we reasoned may also serve as a genetic barrier. We found that the translocation is prevalent in vineyard strains, but absent in oak strains, and presents a post-zygotic barrier to spore viability. Furthermore, the translocation was associated with a fitness cost to the rapid growth rate seen in oak-soil strains. Our results reveal the translocation as a dual-function locus that enforces ecological differentiation while producing a genetic barrier to gene flow in these sympatric populations.},
	language = {en},
	urldate = {2015-08-06},
	journal = {Molecular Biology and Evolution},
	author = {Clowers, Katie J. and Heilberger, Justin and Piotrowski, Jeff S. and Will, Jessica L. and Gasch, Audrey P.},
	month = may,
	year = {2015},
	pmid = {25953281},
	pages = {msv112}
}

