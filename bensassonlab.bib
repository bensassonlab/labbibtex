@article{brown_rapid_2010,
	title = {Rapid expansion and functional divergence of subtelomeric gene families in yeasts},
	volume = {20},
	issn = {1879-0445},
	doi = {10.1016/j.cub.2010.04.027},
	abstract = {BACKGROUND: Subtelomeres, regions proximal to telomeres, exhibit characteristics unique to eukaryotic genomes. Genes residing in these loci are subject to epigenetic regulation and elevated rates of both meiotic and mitotic recombination. However, most genome sequences do not contain assembled subtelomeric sequences, and, as a result, subtelomeres are often overlooked in comparative genomics.
RESULTS: We studied the evolution and functional divergence of subtelomeric gene families in the yeast lineage. Our computational results show that subtelomeric families are evolving and expanding much faster than families that do not contain subtelomeric genes. Focusing on three related subtelomeric MAL gene families involved in disaccharide metabolism that show typical patterns of rapid expansion and evolution, we show experimentally how frequent duplication events followed by functional divergence yield novel alleles that allow the metabolism of different carbohydrates.
CONCLUSIONS: Taken together, our computational and experimental analyses show that the extraordinary instability of eukaryotic subtelomeres supports rapid adaptation to novel niches by promoting gene recombination and duplication followed by functional divergence of the alleles.},
	language = {eng},
	number = {10},
	journal = {Current biology: CB},
	author = {Brown, Chris A. and Murray, Andrew W. and Verstrepen, Kevin J.},
	month = may,
	year = {2010},
	pmid = {20471265},
	pmcid = {PMC2877759},
	keywords = {Animals, Base Sequence, Biological Evolution, Genome, Fungal, Markov Chains, Molecular Sequence Data, Multigene Family, Phylogeny, Telomere, Yeasts},
	pages = {895--903}
}

@article{vidal-leiria_distribution_1979,
	title = {Distribution of the {Maximum} {Temperature} for {Growth} among {Yeasts}},
	volume = {71},
	copyright = {Copyright {\textcopyright} 1979 Mycological Society of America},
	issn = {0027-5514},
	url = {http://www.jstor.org/stable/3759059},
	doi = {10.2307/3759059},
	abstract = {The maximum temperature for growth ({\textless}tex-math{\textgreater}\${\textbackslash}text\{T\}\_\{{\textbackslash}text\{max\}\}\${\textless}/tex-math{\textgreater}) was determined for 594 yeast strains belonging to 112 species of the genera Candida, Torulopsis, Hansenula, Pichia, Metschnikowia and Leucosporidium. Less than 2\% of the strain population was psychrophilic having {\textless}tex-math{\textgreater}\${\textbackslash}text\{T\}\_\{{\textbackslash}text\{max\}\}\${\textless}/tex-math{\textgreater} values below 24 C. More than 98\% consisted of mesophilic strains with {\textless}tex-math{\textgreater}\${\textbackslash}text\{T\}\_\{{\textbackslash}text\{max\}\}\${\textless}/tex-math{\textgreater} values ranging from 26 to 48 C, the highest frequency being in the 34-38 C range. No thermophilic yeasts were encountered. The intraspecific variation of {\textless}tex-math{\textgreater}\${\textbackslash}text\{T\}\_\{{\textbackslash}text\{max\}\}\${\textless}/tex-math{\textgreater} in 41 species of which more than five strains each were studied did not exceed a range of 5 degrees C in 76\% of the species, the highest frequency pertaining to the range of 3 degrees C. Analysis of the {\textless}tex-math{\textgreater}\${\textbackslash}text\{T\}\_\{{\textbackslash}text\{max\}\}\${\textless}/tex-math{\textgreater} variation in the seven species of which more than 15 strains were available brought further evidence that in most yeasts {\textless}tex-math{\textgreater}\${\textbackslash}text\{T\}\_\{{\textbackslash}text\{max\}\}\${\textless}/tex-math{\textgreater} is fixed on the species level within a narrow range of temperatures.},
	number = {3},
	urldate = {2015-08-04},
	journal = {Mycologia},
	author = {Vidal-Leiria, M. and Buckley, H. and Uden, N. van},
	month = may,
	year = {1979},
	pages = {493--501},
	file = {JSTOR Full Text PDF:/Users/douda/Library/Application Support/Firefox/Profiles/8vvwl2pu.default/zotero/storage/PKV84Q3P/Vidal-Leiria et al. - 1979 - Distribution of the Maximum Temperature for Growth.pdf:application/pdf}
}

@incollection{kurtzman_chapter_2011-18,
	address = {London},
	title = {Chapter 85 - {Zygotorulaspora} {Kurtzman} (2003)},
	isbn = {978-0-444-52149-1},
	url = {http://www.sciencedirect.com/science/article/pii/B9780444521491000859},
	abstract = {Publisher Summary
This chapter studies the genus Zygotorulaspora. In the determination of the asexual reproduction it is seen that cell division is by multilateral budding on a narrow base, and budded cells are spherical to ellipsoidal. Pseudohyphae may be present, but true hyphae are not formed. In sexual reproduction it is found that the asci are persistent and may be unconjugated or show conjugation between a cell and its bud or between independent cells. Asci produce one to four smooth, spherical to subspherical ascospores. The species appear to be homothallic. The chapter also discusses physiology/biochemistry and phylogenetic placement of the genus in which glucose and other sugars are fermented. Nitrate is not assimilated, pellicles are absent, coenzyme Q-6 is produced, and the diazonium blue B test is negative. The type species taken is Zygotorulaspora mrakii. Single gene phylogenetic analyses showed Z. florentina and Z. mrakii to be sister species but indicated that they were not species of Zygosaccharomyces. When analyzed in a multigene dataset, Z. florentina and Z. mrakii formed a sister clade with the genus Torulaspora. Although the two species are closely associated with Torulaspora, Kurtzman suggested that they represent a sister genus rather than divergent species of Torulaspora and proposed the genus Zygotorulaspora for Z. florentina and Z. mrakii.},
	booktitle = {The {Yeasts} ({Fifth} {Edition})},
	publisher = {Elsevier},
	author = {Kurtzman, Cletus P.},
