@article{haag-liautard_direct_2007,
	title = {Direct estimation of per nucleotide and genomic deleterious mutation rates in {Drosophila}},
	volume = {445},
	copyright = {{\textcopyright} 2007 Nature Publishing Group},
	issn = {0028-0836},
	url = {http://www.nature.com/nature/journal/v445/n7123/abs/nature05388.html},
	doi = {10.1038/nature05388},
	abstract = {Spontaneous mutations are the source of genetic variation required for evolutionary change, and are therefore important for many aspects of evolutionary biology. For example, the divergence between taxa at neutrally evolving sites in the genome is proportional to the per nucleotide mutation rate, u (ref. 1), and this can be used to date speciation events by assuming a molecular clock. The overall rate of occurrence of deleterious mutations in the genome each generation (U) appears in theories of nucleotide divergence and polymorphism, the evolution of sex and recombination, and the evolutionary consequences of inbreeding. However, estimates of U based on changes in allozymes or DNA sequences and fitness traits are discordant. Here we directly estimate u in Drosophila melanogaster by scanning 20 million bases of DNA from three sets of mutation accumulation lines by using denaturing high-performance liquid chromatography. From 37 mutation events that we detected, we obtained a mean estimate for u of 8.4 10-9 per generation. Moreover, we detected significant heterogeneity in u among the three mutation-accumulation-line genotypes. By multiplying u by an estimate of the fraction of mutations that are deleterious in natural populations of Drosophila, we estimate that U is 1.2 per diploid genome. This high rate suggests that selection against deleterious mutations may have a key role in explaining patterns of genetic variation in the genome, and help to maintain recombination and sexual reproduction.},
	language = {en},
	number = {7123},
	urldate = {2014-10-09},
	journal = {Nature},
	author = {Haag-Liautard, Cathy and Dorris, Mark and Maside, Xulio and Macaskill, Steven and Halligan, Daniel L. and Charlesworth, Brian and Keightley, Peter D.},
	month = jan,
	year = {2007},
	pages = {82--85},
	file = {Full Text PDF:/Users/douda/Library/Application Support/Firefox/Profiles/8vvwl2pu.default/zotero/storage/M9DUW8HI/Haag-Liautard et al. - 2007 - Direct estimation of per nucleotide and genomic de.pdf:application/pdf;Snapshot:/Users/douda/Library/Application Support/Firefox/Profiles/8vvwl2pu.default/zotero/storage/QT2FXPHQ/nature05388.html:text/html}
}

