@article{kurtzman_phylogenetic_2003,
	title = {Phylogenetic circumscription of {Saccharomyces}, {Kluyveromyces} and other members of the {Saccharomycetaceae}, and the proposal of the new genera {Lachancea}, {Nakaseomyces}, {Naumovia}, {Vanderwaltozyma} and {Zygotorulaspora}},
