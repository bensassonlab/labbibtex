@article{deutschbauer_mechanisms_2005,
	title = {Mechanisms of {Haploinsufficiency} {Revealed} by {Genome}-{Wide} {Profiling} in {Yeast}},
	volume = {169},
	url = {http://www.genetics.org/cgi/content/abstract/169/4/1915},
	abstract = {Haploinsufficiency is defined as a dominant phenotype in diploid organisms that are heterozygous for a loss-of-function allele. Despite its relevance to human disease, neither the extent of haploinsufficiency nor its precise molecular mechanisms are well understood. We used the complete set of Saccharomyces cerevisiae heterozygous deletion strains to survey the genome for haploinsufficiency via fitness profiling in rich (YPD) and minimal media to identify all genes that confer a haploinsufficient growth defect. This assay revealed that [{\textasciitilde}]3\% of all [{\textasciitilde}]5900 genes tested are haploinsufficient for growth in YPD. This class of genes is functionally enriched for metabolic processes carried out by molecular complexes such as the ribosome. Much of the haploinsufficiency in YPD is alleviated by slowing the growth rate of each strain in minimal media, suggesting that certain gene products are rate limiting for growth only in YPD. Overall, our results suggest that the primary mechanism of haploinsufficiency in yeast is due to insufficient protein production. We discuss the relevance of our findings in yeast to human haploinsufficiency disorders.},
	number = {4},
	journal = {Genetics},
	author = {Deutschbauer, Adam M. and Jaramillo, Daniel F. and Proctor, Michael and Kumm, Jochen and Hillenmeyer, Maureen E. and Davis, Ronald W. and Nislow, Corey and Giaever, Guri},
	year = {2005},
	pages = {1915--1925},
	annote = {April 1, 2005}
}

