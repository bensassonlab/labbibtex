@article{fang_genome-wide_2012,
	title = {Genome-wide mapping of methylated adenine residues in pathogenic {Escherichia} coli using single-molecule real-time sequencing},
	volume = {30},
	copyright = {{\textcopyright} 2012 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1087-0156},
