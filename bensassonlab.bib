	keywords = {Base Sequence, DNA Transposable Elements, Evolution, Molecular, Gene Dosage, Genetic Variation, Gene Transfer, Horizontal, Genome, Fungal, Genomics, Likelihood Functions, Molecular Sequence Annotation, Nucleotides, Saccharomyces cerevisiae, Sequence Homology, Nucleic Acid, Terminal Repeat Sequences},
