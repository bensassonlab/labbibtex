	annote = {Pertile, Mark DGraham, Alison NChoo, K H AndyKalitsis, PaulResearch Support, Non-U.S. Gov'tUnited StatesGenome researchGenome Res. 2009 Dec;19(12):2202-13. Epub 2009 Sep 8.},
