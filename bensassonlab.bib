	keywords = {*Evolution, Molecular, Base Sequence, Centromere/*genetics, Chromatin Immunoprecipitation, Cloning, Molecular, DNA, Plant/*genetics, Genome, Plant, In Situ Hybridization, Fluorescence, Molecular Sequence Data, Oryza sativa/*genetics, Time Factors},
