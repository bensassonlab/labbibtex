@article{goddard_distinct_2010,
	title = {A distinct population of \textit{{Saccharomyces} cerevisiae} in {New} {Zealand}: evidence for local dispersal by insects and human-aided global dispersal in oak barrels},
	volume = {12},
	copyright = {{\textcopyright} 2009 Society for Applied Microbiology and Blackwell Publishing Ltd},
	issn = {1462-2920},
	shorttitle = {A distinct population of {Saccharomyces} cerevisiae in {New} {Zealand}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1462-2920.2009.02035.x/abstract},
	doi = {10.1111/j.1462-2920.2009.02035.x},
	abstract = {Humans have used S.~cerevisiae to make alcoholic beverages for at least 5000~years and now this super-model research organism is central to advances in our biological understanding. Current models for S.~cerevisiae suggest that its population comprises distinct domesticated and natural groups as well as mosaic strains, but we generally know little of the forces which shape its population structure. In order to test the roles that ecology and geography play in shaping the S.~cerevisiae species we examined nine variable microsatellite loci in 172 strains of S.~cerevisiae isolated from two spontaneous grape juice ferments, soil, flowers, apiaries and bark in New Zealand. Bayesian analysis shows that the S.~cerevisiae in NZ comprise a subdivided but interbreeding population that out-crosses \~{}20\% of the time. Some strains contributing to spontaneous ferments cluster with NZ soil/bark isolates, but others cluster with isolates from French oak barrels. It seems some strains have been globally dispersed by humans in oak barrels while some are locally vectored by insects. These data suggest geography is more important than ecology in shaping S.~cerevisiae's population structure.},
	language = {en},
	number = {1},
	urldate = {2015-09-20},
	journal = {Environmental Microbiology},
	author = {Goddard, Matthew R. and Anfang, Nicole and Tang, Rongying and Gardner, Richard C. and Jun, Casey},
	month = jan,
	year = {2010},
	pages = {63--73},
	file = {Full Text PDF:/Users/douda/Library/Application Support/Firefox/Profiles/8vvwl2pu.default/zotero/storage/XK65B62W/Goddard et al. - 2010 - A distinct population of Saccharomyces cerevisiae .pdf:application/pdf;Snapshot:/Users/douda/Library/Application Support/Firefox/Profiles/8vvwl2pu.default/zotero/storage/596B9D7Q/abstract.html:text/html}
