	pages = {e6700},
	annote = {19693276},
	annote = {eng},
	annote = {Teytelman, LeonidOzaydin, BilgeZill, OliverLefrancois, PhilippeSnyder, MichaelRine, JasperEisen, Michael BGM31105/GM/NIGMS NIH HHS/United StatesHG002779/HG/NHGRI NIH HHS/United StatesResearch Support, N.I.H., ExtramuralUnited StatesPloS onePLoS One. 2009 Aug 20;4(8):e6700.},
